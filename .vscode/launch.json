{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch index.html",
            "type" : "chrome",
            "request": "launch",
            "file": "${workspaceRoot}/index.html"
        }
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost/index.html",
            "sourcemaps" : true,
            "webRoot": "wwwroot"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
        }
    ]
}